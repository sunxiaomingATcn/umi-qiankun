{
  "name": "qiankun-example",
  "version": "0.0.1",
  "description": "qiankun demo",
  "main": "index.js",
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:main": "cd main && npm i",
    "install:sub-vue": "cd sub-vue && npm i",
    "install:sub-react": "cd sub-react && npm i",
    "install:sub-umi": "cd sub-umi && npm i",
    "install:sub-umi-product": "cd sub-umi-product && npm i",
    "start": "npm-run-all --parallel start:*",
    "start:main": "cd main && npm start",
    "start:sub-react": "cd sub-react && npm start",
    "start:sub-vue": "cd sub-vue && npm start",
    "start:sub-umi": "cd sub-umi && npm start",
    "start:sub-umi-product": "cd sub-umi-product && npm start",
    "build": "npm-run-all build:* && bash ./scripts/bundle.sh",
    "build:main": "cd main && npm run build",
    "build:sub-react": "cd sub-react && npm run build",
    "build:sub-vue": "cd sub-vue && npm run build",
    "build:sub-umi": "cd sub-umi && npm run build",
    "build:sub-umi-product": "cd sub-umi-product && npm run build",
    "build-micro:sub-umi-product": "cd sub-umi-product && npm run build && bash ./scripts/bundle.sh",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint-staged": "lint-staged",
    "lint:style": "stylelint --fix \"./**/**/*.less\" --syntax less",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty **/src/**/*.* && npm run lint:style"
  },
  "dependencies": {
    "@babel/core": "^7.19.3"
  },
  "devDependencies": {
    "@umijs/fabric": "^3.0.0",
    "connect-history-api-fallback": "^2.0.0",
    "express": "^4.18.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
